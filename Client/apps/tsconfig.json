{
    "typeAcquisition": {
        "enable": true,
        "include": [ "jquery" ]
    },
    "include": [
        "**/*.ts",
    ],
    "references": [
        { "path": "../storyteller/tsconfig.json" }
    ],
    "compilerOptions": {
        "noImplicitAny": false,
        "noEmitOnError": false,
        "removeComments": false,
        "sourceMap": false,
        "target": "ES2022",
        "module": "esnext",
        "rootDir": ".",
        "outDir": "../../docs",
    },
    "compileOnSave": true,
}
